<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liurui.sys.user.mapper.UserMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.liurui.sys.user.entity.User">
        <id column="ids" property="ids" />
    <result column="del_flag" property="delFlag" />
        <result column="account" property="account" />
        <result column="password" property="password" />
        <result column="real_name" property="realName" />
        <result column="sex" property="sex" />
        <result column="age" property="age" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="address" property="address" />
        <result column="nick_name" property="nickName" />
        <result column="login_flag" property="loginFlag" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ids, account, password, real_name, sex, age, phone, email, address, nick_name, login_flag
    </sql>

</mapper>
