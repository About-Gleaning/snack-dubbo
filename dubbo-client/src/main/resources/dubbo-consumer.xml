<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 消费方应用名 -->
    <dubbo:application name="${dubbo.application.name}"/>
    <!-- 注册中心服务地址 -->
    <dubbo:registry id="zookeeper" protocol="zookeeper" address="${dubbo.registry.address}"/>
    <!--调用服务-->
    <dubbo:reference id="sysRoleService" interface="com.liurui.base.sys.service.ISysRoleService" check="false" registry="zookeeper" protocol="dubbo" timeout="15000"/>
    <dubbo:reference id="sysUserService" interface="com.liurui.base.sys.service.ISysUserService" check="false" registry="zookeeper" protocol="dubbo" timeout="15000"/>
    <dubbo:reference id="sysUserRoleService" interface="com.liurui.base.sys.service.ISysUserRoleService" check="false" registry="zookeeper" protocol="dubbo" timeout="15000"/>
    <dubbo:reference id="sysPermissionService" interface="com.liurui.base.sys.service.ISysPermissionService" check="false" registry="zookeeper" protocol="dubbo" timeout="15000"/>
    <dubbo:reference id="sysRolePermissionService" interface="com.liurui.base.sys.service.ISysRolePermissionService" check="false" registry="zookeeper" protocol="dubbo" timeout="15000"/>
</beans>